{

    "version": "0.2.0",
    "configurations": [      
        
        
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },

        {
            "name": "Deno",
            "type": "node",
            "request": "attach",
            "cwd": "${workspaceFolder}",
            "port": 9229
        }

    ]
}